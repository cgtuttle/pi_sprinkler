<%= form_for(@station) do |f| %>
  <%= render partial: "layouts/errors", locals: {resource: @station} %>
  <div class="row">
    <div class="form-group col-md-1">
      <%= f.label :number %>
      <%= f.text_field :number, class: "form-control" %>
    </div>
    <div class="form-group col-md-5">
      <%= f.label :location %>
      <%= f.text_field :location, class: "form-control" %>
    </div>
    <div class="form-group col-md-1">
      <%= f.label :sequence %>
      <%= f.number_field :sequence, class: "form-control" %>
    </div>
    <div class="form-group col-md-1">
      <%= f.label :duration %>
      <%= f.number_field :duration, class: "form-control" %>
    </div>
  </div>
  <div class="row">
    <div class="form-group col-md-12">
      <%= f.submit %>
    </div>
  </div>
<% end %>
